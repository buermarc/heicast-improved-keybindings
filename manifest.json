{

  "manifest_version": 2,
  "name": "heicast_improved_keybindings",
  "version": "0.0.5",

  "description": "Add custom keybindings",

  "content_scripts": [
    {
      "matches": ["*://heicast.uni-heidelberg.de/*"],
      "js": ["custom_bindings.js"]
    }
  ],
  "browser_specific_settings": {
      "gecko": {
          "id": "heicast_improved_keybindings@googlemail.com"
      }
  }
}
